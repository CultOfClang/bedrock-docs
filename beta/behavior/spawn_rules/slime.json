{
    "format_version": "1.19.10",
    "minecraft:spawn_rules": {
      "description": {
        "identifier": "minecraft:slime",
        "population_control": "monster"
      },
      "conditions": [
        {
          "minecraft:spawns_on_surface": {},
          "minecraft:spawns_underground": {},
          "minecraft:difficulty_filter": {
            "min": "easy",
            "max": "hard"
          },
          "minecraft:weight": {
            "default": 100
          },
          "minecraft:height_filter": {
            "min": -64,
            "max": 40
          },
          "minecraft:biome_filter": {
            "any_of": [
              { 
                "all_of": [
                  {
                    "test": "has_biome_tag",
                    "operator": "==",
                    "value": "monster"
                  },
                  {
                    "test": "has_biome_tag",
                    "operator": "!=",
                    "value": "swamp"
                  },
                  {
                    "test": "has_biome_tag",
                    "operator": "!=",
                    "value": "mangrove_swamp"
                  }
                ]
              },
              {
                "test": "has_biome_tag",
                "operator": "==",
                "value": "frozen"
              }
            ]
          }
        },
        {
          "minecraft:spawns_on_surface": {},
          "minecraft:spawns_underground": {},
          "minecraft:difficulty_filter": {
            "min": "easy",
            "max": "hard"
          },
          "minecraft:weight": {
            "default": 100
          },
          "minecraft:height_filter": {
            "min": 50,
            "max": 70
          },
          "minecraft:biome_filter": {
            "any_of": [
              {
                "test": "has_biome_tag",
                "operator": "==",
                "value": "swamp"
              },
              {
                "test": "has_biome_tag",
                "operator": "==",
                "value": "mangrove_swamp"
              }
            ]
          }
        }
      ]
    }
  }
